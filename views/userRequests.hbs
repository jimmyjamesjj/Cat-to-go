<a href="/"> Home </a>
<p></p>
<div class="userrequests">
<h1 ><i>list of client requests</i></h1>
{{this.fname}}

{{#each result}}
        <div class="numberofcats">
            <h5>number of cats: {{this.number_of_cats}}</h5>
            </div>
         <div class="catsize">
            <h5>Cat size: {{this.catsize}}</h5>
            </div>   
         <div class="numberofnights">
            <h5>Number of nights: {{this.number_of_nights}}</h5>
            </div> 
         <div class="date">
            <h5>Date: {{this.date}}</h5>
            </div>
         <div class="phonenumber">
            <h5>Owner Phone number: {{this.phonenumber}}</h5>
            </div>
         <div class="owneraddress">
            <h5>Owner address: {{this.owneraddress}}</h5>
            </div>
<div><a href='/views/{{this._id}}/edit' class="btn btn-primary">confirm client booking</a></div>



<p></p>
         <form action="/views/{{this._id}}/delete" method="POST">
                <button class="btn btn-primary">Cancel client booking</button>

   
{{/each}}

</div>